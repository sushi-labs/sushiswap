                    }
                }
            }
            else if (node.type === 'VariableDeclaration' && parent && !ast_helpers_1.isVarLoop(parent) && job.analysis.evaluatePureExpressions) {
                for (const decl of node.declarations) {
                    if (!decl.init)
                        continue;
