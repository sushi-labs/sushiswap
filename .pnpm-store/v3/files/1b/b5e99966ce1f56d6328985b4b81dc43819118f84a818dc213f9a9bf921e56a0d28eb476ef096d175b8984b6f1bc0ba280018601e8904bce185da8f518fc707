/**
 * Load file
 * @param path
 * @param throwError
 * @returns
 */
export declare const loadJSON: <T>(path: string, throwError?: boolean) => Promise<T | undefined>;
/**
 * Export file
 * @param filePath
 * @param content
 * @returns
 */
export declare const exportFile: (filePath: string, content: string) => Promise<any>;
