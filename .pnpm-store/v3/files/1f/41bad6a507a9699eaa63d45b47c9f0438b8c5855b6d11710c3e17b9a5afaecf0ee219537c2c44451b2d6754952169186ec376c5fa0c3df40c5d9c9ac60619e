import { ImageProps, ViewProps, TextProps } from 'react-native';
import { ComponentClass, ReactNode } from 'react';
export declare const primitives: {
    View: ComponentClass<ViewProps & {
        children?: ReactNode;
    }, any>;
    Text: ComponentClass<TextProps & {
        children?: ReactNode;
    }, any>;
    Image: ComponentClass<ImageProps, any>;
};
