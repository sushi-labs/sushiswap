import { UseDragOptions } from '../useDrag';
/** Restrict a point to an area, or samples along a path. */
export default function restrictPoint(point: {
    x: number;
    y: number;
}, samples: DOMPoint[], restrict?: UseDragOptions['restrict']): {
    x: number;
    y: number;
};
