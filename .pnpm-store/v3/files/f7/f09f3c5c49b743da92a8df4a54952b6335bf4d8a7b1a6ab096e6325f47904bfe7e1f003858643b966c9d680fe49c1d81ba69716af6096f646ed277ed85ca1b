/**
 * @param {ArrayBufferView|ArrayBuffer} bytes
 * @returns {string}
 */
export function decodeText(bytes: ArrayBufferView | ArrayBuffer): string;
/**
 * @param {string} text
 * @returns {Uint8Array}
 */
export function encodeText(text: string): Uint8Array;
/**
 * Returns a new Uint8Array created by concatenating the passed Arrays
 *
 * @param {Array<ArrayLike<number>>} arrs
 * @param {number} length
 * @returns {Uint8Array}
 */
export function concat(arrs: Array<ArrayLike<number>>, length: number): Uint8Array;
