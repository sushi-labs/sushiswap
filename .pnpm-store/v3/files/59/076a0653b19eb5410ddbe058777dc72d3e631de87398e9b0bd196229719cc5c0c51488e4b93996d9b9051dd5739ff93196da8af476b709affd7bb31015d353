    throw new errors_1.NowBuildError({
        code: `BUILD_UTILS_GET_NODE_BIN_PATH`,
        message: `Running \`npm bin\` failed to return a valid bin path (code=${code}, stdout=${stdout}, stderr=${stderr})`,
    });
}
