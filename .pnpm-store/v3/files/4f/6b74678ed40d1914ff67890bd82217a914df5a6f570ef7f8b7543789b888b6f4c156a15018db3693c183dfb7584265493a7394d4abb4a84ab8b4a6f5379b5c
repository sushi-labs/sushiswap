import type { Offer } from 'src/types';
export declare function setOffer(offer?: Offer): {
    '@type': string;
    priceSpecification: {
        '@type': string;
        priceCurrency: string;
        price: string;
    } | undefined;
    itemOffered: {
        '@type': string;
        name: string;
        description: string;
    } | undefined;
} | undefined;
