{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "generate": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".graphclient/**",
        ".mesh/**",
        "typechain/**"
      ],
      "cache": true
    },
    "build": {
      "outputs": [
        ".next/**",
        ".graphclient/**",
        ".mesh/**",
        "artifacts/**",
        "cache/**",
        "dist/**",
        "typechain/**"
      ],
      "dependsOn": [
        "^build",
        "generate"
      ],
      "env": [
        "NEXT_PUBLIC_ALCHEMY_ID",
        "NEXT_PUBLIC_INFURA_ID",
        "NEXT_PUBLIC_STRAPI_API_URL",
        "LIST_PR_WEBHOOK_URL",
        "OCTOKIT_KEY",
        "ANALYTICS_URL",
        "BLOG_URL",
        "EARN_URL",
        "FURO_URL",
        "PARTNER_URL",
        "SWAP_URL",
        "XSWAP_URL"
      ],
      "cache": true
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "dependsOn": [
        "^build",
        "generate"
      ],
      "cache": false
    },
    "test": {
      "outputs": [
        "coverage/**"
      ],
      "dependsOn": [
        "^build",
        "generate"
      ],
      "cache": true
    },
    "lint": {
      "outputs": [],
      "dependsOn": []
    },
    "validate": {
      "cache": false
    },
    "export": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "exports.json"
      ],
      "cache": true
    },
    "node": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "deployments/**"
      ],
      "cache": true
    },
    "server": {
      "dependsOn": [
        "build"
      ],
      "cache": true
    }
  }
}
