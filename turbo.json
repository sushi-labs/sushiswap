{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "db:init": {
      "outputs": [
        "prisma/**"
      ],
      "cache": false
    },
    "db:pull": {
      "outputs": [
        "prisma/**"
      ],
      "dependsOn": [
        "db:init"
      ],
      "cache": false,
      "env": [
        "DATABASE_URL"
      ]
    },
    "db:generate": {
      "dependsOn": [
        "db:pull"
      ],
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "playwright": {
      "cache": false
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": [
        "coverage/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "start": {
      "dependsOn": [
        "^build",
        "^db:generate",
        "generate"
      ],
      "cache": false
    },
    "generate": {
      "outputs": [
        ".graphclient/**",
        ".mesh/**",
        "typechain/**",
        "artifacts/**",
        "cache/**"
      ]
    },
    "build": {
      "outputs": [
        ".next/**",
        ".graphclient/**",
        ".mesh/**",
        "dist/**",
        "typechain/**",
        "artifacts/**",
        "cache/**",
        "public/**"
      ],
      "dependsOn": [
        "^build",
        "^db:generate",
        "db:generate",
        "generate"
      ],
      "env": [
        "ROOT_URL",
        "ACADEMY_URL",
        "ANALYTICS_URL",
        "BLOG_URL",
        "BRIDGE_URL",
        "EARN_URL",
        "FURO_URL",
        "INTERNAL_URL",
        "KASHI_URL",
        "PARTNER_URL",
        "SWAP_URL",
        "XSWAP_URL",
        "DATABASE_URL",
        "REDIS_URL"
      ]
    },
    "clean": {
      "cache": false
    },
    "check": {
      "cache": false
    },
    "dev": {
      "dependsOn": [
        "^build",
        "^db:generate",
        "generate"
      ],
      "cache": false
    },
    "validate": {
      "cache": false
    },
    "anvil": {
      "cache": false,
      "env": [
        "ANVIL_FORK_URL"
      ]
    },
    "export": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "exports.json"
      ],
      "cache": true
    },
    "node": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "deployments/**"
      ]
    },
    "server": {
      "dependsOn": [
        "^build"
      ]
    }
  }
}
