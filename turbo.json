{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "generate": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".graphclient/**",
        "typechain/**"
      ]
    },
    "build": {
      "outputs": [
        ".graphclient/**",
        ".next/**",
        "artifacts/**",
        "cache/**",
        "dist/**",
        "typechain/**"
      ],
      "dependsOn": [
        "^build",
        "generate"
      ],
      "env": [
        "NEXT_PUBLIC_ALCHEMY_ID",
        "NEXT_PUBLIC_INFURA_ID",
        "INFURA_ID",
        "ALCHEMY_ID"
      ]
    },
    "root#build": {
      "outputs": [
        ".next/**"
      ],
      "dependsOn": [
        "^build",
        "generate"
      ],
      "env": [
        "ANALYTICS_URL",
        "BLOG_URL",
        "EARN_URL",
        "FURO_URL",
        "PARTNER_URL",
        "SWAP_URL",
        "XSWAP_URL"
      ]
    },
    "blog#generate": {
      "outputs": [
        ".mesh/**"
      ]
    },
    "blog#build": {
      "outputs": [
        ".next/**"
      ],
      "dependsOn": [
        "^build",
        "blog#generate"
      ],
      "env": [
        "NEXT_PUBLIC_STRAPI_API_URL",
        "STRAPI_PREVIEW_SECRET"
      ]
    },
    "internal#build": {
      "outputs": [
        ".next/**"
      ],
      "dependsOn": [
        "^build",
        "generate"
      ],
      "env": [
        "OCTOKIT_KEY",
        "PARTNER_URL"
      ]
    },
    "partner#build": {
      "outputs": [
        ".next/**"
      ],
      "dependsOn": [
        "^build",
        "generate"
      ],
      "env": [
        "LIST_PR_WEBHOOK_URL",
        "OCTOKIT_KEY"
      ]
    },
    "farms-script#generate": {
      "outputs": [
        ".graphclient/**"
      ]
    },
    "farms-script#build": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "^build",
        "farms-script#generate"
      ],
      "env": [
        "REDIS_URL",
        "ALCHEMY_ID"
      ]
    },
    "@sushiswap/tenderly#build": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "^build"
      ],
      "env": [
        "TENDERLY_ACCESS_KEY"
      ]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "dependsOn": [
        "^build",
        "generate"
      ],
      "cache": false
    },
    "test": {
      "outputs": [
        "coverage/**"
      ],
      "dependsOn": [
        "^build",
        "generate"
      ]
    },
    "lint": {
      "outputs": [],
      "dependsOn": []
    },
    "validate": {
      "cache": false
    },
    "export": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "exports.json"
      ],
      "cache": true
    },
    "node": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "deployments/**"
      ]
    },
    "server": {
      "dependsOn": [
        "build"
      ]
    }
  },
  "globalEnv": ["NODE_ENV"]
}
