{
  "extends": ["@tsconfig/strictest/tsconfig", "@tsconfig/esm/tsconfig"],
  "include": ["."],
  "exclude": ["node_modules", ".vercel"],
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "nodenext",
  }
}
