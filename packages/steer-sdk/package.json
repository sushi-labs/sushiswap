{
  "name": "@sushiswap/steer-sdk",
  "version": "0.0.0",
  "private": true,
  "description": "Sushi Steer SDK",
  "keywords": [
    "sushi",
    "steer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sushiswap/sushiswap.git",
    "directory": "packages/steer-sdk"
  },
  "license": "MIT",
  "author": "LufyCZ <me@lufy.cz>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./abi": {
      "types": "./dist/abi/index.d.ts",
      "import": "./dist/abi/index.js",
      "default": "./dist/abi/index.js"
    },
    "./package.json": "./package.json"
  },
  "module": "./dist/index.js",
  "source": "src/index.ts",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "src/index"
      ],
      "abi": [
        "src/abi/index"
      ]
    }
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc",
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsc --watch",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@steerprotocol/contracts": "2.1.0-beta.14",
    "sushi": "4.1.16",
    "viem": "2.21.4"
  },
  "devDependencies": {
    "@tsconfig/esm": "1.0.4",
    "@tsconfig/strictest": "2.0.2",
    "@types/node": "20",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.7.2"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  }
}
