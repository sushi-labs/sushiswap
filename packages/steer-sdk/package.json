{
  "name": "@sushiswap/steer-sdk",
  "version": "0.0.0",
  "description": "Sushi Steer SDK",
  "keywords": [
    "sushi",
    "steer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sushiswap/sushiswap.git",
    "directory": "packages/steer-sdk"
  },
  "license": "MIT",
  "author": "LufyCZ <me@lufy.cz>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./abi": {
      "types": "./dist/abi/index.d.ts",
      "import": "./dist/abi/index.js",
      "default": "./dist/abi/index.js"
    },
    "./package.json": "./package.json"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "src/index"
      ],
      "abi": [
        "src/abi/index"
      ]
    }
  },
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/abi/index.ts",
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsc --watch",
    "lint": "TIMING=1 eslint src",
    "lint:fix": "TIMING=1 eslint src --fix",
    "prepublishOnly": "pnpm build"
  },
  "jest": {
    "preset": "@sushiswap/jest-config/node"
  },
  "dependencies": {
    "@steerprotocol/contracts": "2.1.0-beta.14",
    "@sushiswap/client": "workspace:*",
    "@sushiswap/database": "workspace:*",
    "@whatwg-node/fetch": "0.8.4",
    "sushi": "workspace:*",
    "tslib": "latest",
    "viem": "1.21.4"
  },
  "devDependencies": {
    "@sushiswap/jest-config": "workspace:*",
    "@tsconfig/esm": "1.0.4",
    "@tsconfig/strictest": "2.0.2",
    "@types/node": "18",
    "@wagmi/core": "1.4.13",
    "eslint": "8.43.0",
    "next": "14.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "7.1.0",
    "typescript": "5.1.6"
  },
  "peerDependencies": {
    "@wagmi/core": "1.4.13",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "wagmi": "1.4.13"
  },
  "peerDependenciesMeta": {
    "@wagmi/core": {
      "optional": false
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "wagmi": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "next": "14.1.0"
  }
}
