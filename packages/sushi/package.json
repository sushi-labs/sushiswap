      "import": "./dist/_esm/validate/index.js",
      "default": "./dist/_cjs/validate/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/_cjs/index.js",
  "module": "./dist/_esm/index.js",
  "types": "./dist/_types/index.d.ts",
  "typesVersions": {
    "*": {
      "abi": [
        "./dist/_types/abi/index.d.ts"
      ],
      "address": [
        "./dist/_types/address/index.d.ts"
      ],
      "bigint-serializer": [
        "./_types/bigint-serializer/index.d.ts"
      ],
      "calculate": [
        "./dist/_types/calculate/index.d.ts"
      ],
      "chain": [
        "./dist/_types/chain/index.d.ts"
      ],
      "config": [
        "./dist/_types/config/index.d.ts"
      ],
      "config/subgraph": [
        "./dist/_types/config/subgraph/index.d.ts"
      ],
      "contract": [
        "./dist/_types/contract/index.d.ts"
      ],
      "convert": [
        "./dist/_types/convert/index.d.ts"
      ],
      "currency": [
        "./dist/_types/currency/index.d.ts"
      ],
      "data": [
        "./dist/_types/data/index.d.ts"
      ],
      "dex": [
        "./dist/_types/dex/index.d.ts"
      ],
      "format": [
        "./dist/_types/format/index.d.ts"
      ],
      "log": [
        "./dist/_types/log/index.d.ts"
      ],
      "math": [
        "./dist/_types/math/index.d.ts"
      ],
      "pool": [
        "./dist/_types/pool/index.d.ts"
      ],
      "router": [
        "./dist/_types/router/index.d.ts"
      ],
      "sort": [
        "./dist/_types/sort/index.d.ts"
      ],
      "token-list": [
        "./dist/_types/token-list/index.d.ts"
      ],
      "types": [
        "./dist/_types/types/index.d.ts"
      ],
      "validate": [
        "./dist/_types/validate/index.d.ts"
      ]
    }
  },
  "typings": "./dist/_types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:cjs && pnpm build:esm && pnpm build:types",
    "build:cjs": "tsc --project ./tsconfig.build.json --module commonjs --outDir ./dist/_cjs --removeComments --verbatimModuleSyntax false --esModuleInterop true && printf '{\"type\":\"commonjs\"}' > ./dist/_cjs/package.json",
    "build:esm": "tsc --project ./tsconfig.build.json --module es2020 --outDir ./dist/_esm && printf '{\"type\": \"module\",\"sideEffects\": \"true\"}' > ./dist/_esm/package.json",
    "build:types": "tsc --project ./tsconfig.build.json --module esnext --declarationDir ./dist/_types --emitDeclarationOnly --declaration --declarationMap",
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsc -w",
    "generate": "npx tsx ./scripts/generate.ts",
    "prepublishOnly": "pnpm build",
    "test": "vitest run -c ./test/vitest.config.ts",
    "test:debug": "vitest --inspect-brk --no-threads run -c ./test/vitest.config.ts",
    "test:watch": "vitest dev -c ./test/vitest.config.ts"
  },
  "dependencies": {
    "@uniswap/token-lists": "1.0.0-beta.33",
    "big.js": "6.1.1",
    "date-fns": "3.3.1",
    "decimal.js-light": "2.5.1",
    "seedrandom": "3.0.5",
    "tiny-invariant": "1.3.1",
    "toformat": "2.0.0",
    "tsx": "^4.7.1"
  },
  "devDependencies": {
    "@tsconfig/node20": "20.1.4",
    "@tsconfig/strictest": "2.0.2",
    "@types/big.js": "6.2.0",
    "@types/seedrandom": "3.0.1",
    "tsc-alias": "^1.8.10",
    "typescript": "5.5.4",
    "viem": "2.21.4",
    "vitest": "0.34.6",
    "zod": "3.23.8"
  },
  "peerDependencies": {
    "viem": "*",
    "zod": "*"
  },
  "peerDependenciesMeta": {
    "viem": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  }
}
