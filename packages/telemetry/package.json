    "@types/react-dom": "19.0.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "typescript": "5.7.2"
  },
  "peerDependencies": {
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
