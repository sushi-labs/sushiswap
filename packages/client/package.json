{
  "name": "@sushiswap/client",
  "version": "0.0.0",
  "description": "Sushi Client",
  "keywords": [
    "sushi",
    "client"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sushiswap/sushiswap.git",
    "directory": "packages/client"
  },
  "license": "MIT",
  "author": "LufyCZ <me@lufy.cz>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js",
      "default": "./dist/hooks/index.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.js",
      "default": "./dist/api/index.js"
    },
    "./package.json": "./package.json"
  },
  "module": "./dist/index.js",
  "source": "src/index.ts",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "src/index"
      ],
      "hooks": [
        "src/hooks/index"
      ],
      "api": [
        "src/api/index"
      ]
    }
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc",
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsc --watch",
    "lint": "TIMING=1 eslint src",
    "lint:fix": "TIMING=1 eslint src --fix",
    "prepublishOnly": "pnpm build"
  },
  "jest": {
    "preset": "@sushiswap/jest-config/node"
  },
  "dependencies": {
    "@sushiswap/bonds-sdk": "workspace:*",
    "@sushiswap/database": "workspace:*",
    "@sushiswap/graph-client": "workspace:*",
    "@sushiswap/steer-sdk": "workspace:*",
    "@whatwg-node/fetch": "0.8.4",
    "deepmerge-ts": "^4.3.0",
    "tslib": "latest",
    "viem": "2.21.35",
    "zod": "3.22.0"
  },
  "devDependencies": {
    "@sushiswap/jest-config": "workspace:*",
    "@sushiswap/tokens-api": "workspace:*",
    "@sushiswap/typescript-config": "workspace:*",
    "@tsconfig/esm": "1.0.4",
    "@tsconfig/strictest": "2.0.2",
    "@types/node": "20",
    "eslint": "8.43.0",
    "react": "18.2.0",
    "sushi": "workspace:*",
    "swr": "2.1.5",
    "tsup": "7.2.0",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "react": "*",
    "sushi": "*",
    "swr": "*"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "sushi": {
      "optional": true
    },
    "swr": {
      "optional": true
    }
  }
}
