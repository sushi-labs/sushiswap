    "@types/react-virtualized-auto-sizer": "1.0.1",
    "@types/react-window": "1.8.5",
    "autoprefixer": "10.4.14",
    "eslint": "8.43.0",
    "next": "13.4.19",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "tsup": "7.1.0",
    "typescript": "5.1.6",
    "wagmi": "1.4.2"
  },
  "peerDependencies": {
    "next": "13.4.19",
    "react": "18.2.0",
