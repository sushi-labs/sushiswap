    "@sushiswap/eslint-config": "workspace:*",
    "@sushiswap/jest-config": "workspace:*",
    "@sushiswap/math": "workspace:*",
    "@sushiswap/prettier-config": "workspace:*",
    "@sushiswap/typescript-config": "workspace:*",
    "@types/jest": "^29.2.0",
    "eslint": "8.29.0",
    "jest": "29.3.1",
    "tiny-invariant": "1.3.1",
    "tsup": "6.5.0",
    "typescript": "4.9.4"
  }
