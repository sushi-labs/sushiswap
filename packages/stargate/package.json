{
  "name": "@sushiswap/stargate",
  "version": "0.1.0",
  "description": "Sushi Stargate",
  "keywords": [
    "sushi",
    "stargate"
  ],
  "license": "MIT",
  "author": "Matthew Lilley <hello@matthewLilley.com>",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/**",
    "abis/**",
    "IStargateReceiver.sol",
    "IStargateRouter.sol"
  ],
  "scripts": {
    "build": "tsc",
