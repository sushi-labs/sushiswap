{
  "name": "@sushiswap/bonds-sdk",
  "version": "0.0.0",
  "private": true,
  "description": "Sushi Bonds SDK",
  "keywords": [
    "sushiswap",
    "bonds",
    "sdk"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sushiswap/sushiswap.git",
    "directory": "packages/bonds-sdk"
  },
  "license": "MIT",
  "author": "Lufy <me@lufy.cz>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./abi": {
      "types": "./dist/abi/index.d.ts",
      "import": "./dist/abi/index.js",
      "default": "./dist/abi/index.js"
    },
    "./package.json": "./package.json"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "src/index"
      ],
      "abi": [
        "src/abi/index"
      ]
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc",
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsc -w",
    "prepublishOnly": "pnpm build",
    "test": "jest --passWithNoTests"
  },
  "jest": {
    "preset": "@sushiswap/jest-config"
  },
  "dependencies": {
    "sushi": "workspace:*"
  },
  "devDependencies": {
    "@sushiswap/jest-config": "workspace:*",
    "@tsconfig/esm": "1.0.4",
    "@tsconfig/strictest": "2.0.2",
    "@types/jest": "29.5.5",
    "@types/node": "20",
    "jest": "29.7.0",
    "ts-jest": "29.1.1",
    "typescript": "5.5.4",
    "viem": "2.21.4"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "next": "14.2.11"
  }
}
