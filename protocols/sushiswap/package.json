  "keywords": [
    "sushi",
    "swap"
  ],
  "homepage": "https://github.com/sushiswap/sushiswap/protocols/sushiswap#readme",
  "bugs": {
    "url": "https://github.com/sushiswap/sushiswap/issues"
  },
  "repository": {
    "type": "git",
